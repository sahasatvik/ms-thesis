    \entry{pintado-romo-2011}{article}{}
      \name{author}{2}{}{%
        {{un=0,uniquepart=base,hash=3e9df13fb855de948844c7a01bad00bc}{%
           family={López-Pintado},
           familyi={L\bibinithyphendelim P\bibinitperiod},
