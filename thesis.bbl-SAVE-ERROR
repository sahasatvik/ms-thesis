      \strng{namehash}{7bbd3f662b7eedb6c587210ee6554441}
      \strng{fullhash}{7bbd3f662b7eedb6c587210ee6554441}
      \strng{bibnamehash}{7bbd3f662b7eedb6c587210ee6554441}
      \strng{authorbibnamehash}{7bbd3f662b7eedb6c587210ee6554441}
