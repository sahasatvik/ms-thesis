      \strng{fullhash}{57e7a8ae30a78ce0ab0a9de6df49cadd}
      \strng{bibnamehash}{57e7a8ae30a78ce0ab0a9de6df49cadd}
      \strng{authorbibnamehash}{57e7a8ae30a78ce0ab0a9de6df49cadd}
