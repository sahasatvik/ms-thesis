      \strng{bibnamehash}{8d1f25c7b176dc48a6efdb580e38079f}
      \strng{authorbibnamehash}{8d1f25c7b176dc48a6efdb580e38079f}
      \strng{authornamehash}{8d1f25c7b176dc48a6efdb580e38079f}
